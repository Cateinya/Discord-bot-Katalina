{
  "name": "discord-bot",
  "version": "0.0.1",
  "description": "a bot for Discord",
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint . --ext .js --ext .json",
    "start": "node index.js"
  },
  "dependencies": {
    "cleverbot.io": "^1.0.4",
    "curl-to-json": "^1.0.24",
    "discord.js": "github:discordjs/discord.js",
    "discord.js-commando": "github:discordjs/Commando",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "request": "^2.88.0"
  },
  "keywords": [
    "node",
    "discord",
    "bot"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-amber": "^2.0.0",
    "eslint-plugin-json": "^1.4.0",
    "typescript": "^2.6.2"
  },
  "eslintConfig": {
    "extends": "amber",
    "plugins": [
      "json"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
